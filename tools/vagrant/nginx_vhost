# "./tools/vagrant/ngnix_vhost"

# develop
server {
    listen 8020;
    listen [::]:8020 ipv6only=on;
    server_name localhost;

    root /var/www;
    index index.html;

    # Important for VirtualBox
    sendfile off;

    # Gzip Settings
    gzip on;
    gzip_disable "msie6";

    location / {
        try_files $uri $uri/ =404;
        autoindex on;
    }

}
